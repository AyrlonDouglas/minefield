{
    "version": 2,
    "env": {},
    "builds": [
      {
        "src": "src/app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/app.js",
        "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"]
      },
      {
        "src": "/author/(.*)",
        "dest": "src/app.js",
        "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"]
      },
      {
        "src": "/level/(.*)",
        "dest": "src/app.js",
        "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"]
      },
      {
        "src": "/game/(.*)",
        "dest": "src/app.js",
        "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"]
      },
      {
        "src": "/gaming/(.*)",
        "dest": "src/app.js",
        "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"]
      },
      {
        "src": "/lose1/(.*)",
        "dest": "src/app.js",
        "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"]
      },
      {
        "src": "/lose/(.*)",
        "dest": "src/app.js",
        "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"]
      },
      {
        "src": "/victory/(.*)",
        "dest": "src/app.js",
        "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"]
      }
    ]
  }