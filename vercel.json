{
    "name":"Minefield",
    "version": 2,
    "public":true,
    "env": {},
    "builds": [
      {
        "src": "src/app.js",
        "use": "@vercel/node"
      }
      
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/public/$1"},
      {
        "src": "/(.*)",
        "dest": "src/app.js",
        "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"]
      },
    ]
  }